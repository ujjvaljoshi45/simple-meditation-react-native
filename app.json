{
  "expo": {
    "name": "simple-meditation",
    "slug": "simple-meditation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/simpleMeditationLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/simpleMeditationLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ujjvaljoshi45.simplemeditation"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/simpleMeditationLogo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0cbd8c2d-ed83-4d3b-9ecf-888b24d44aef"
      }
    }
  }
}
